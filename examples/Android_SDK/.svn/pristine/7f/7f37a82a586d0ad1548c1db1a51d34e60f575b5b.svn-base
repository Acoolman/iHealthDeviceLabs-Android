<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.ihealth.sdk.AM3"
    tools:showIn="@layout/activity_am3"
    android:orientation="vertical">

    <Button
        android:id="@+id/getbattery"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="getBattery" />

    <Button
        android:id="@+id/getuserid"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="getUserId" />

    <Button
        android:id="@+id/getalarmnum"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="getAlarnNum" />

    <Button
        android:id="@+id/setalarm"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="setAlarm" />

    <Button
        android:id="@+id/syncactivity"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="syncActivity" />

    <Button
        android:id="@+id/syncreal"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="syncReal" />

    <Button
        android:id="@+id/getuserinfo"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="getUserInfo" />

    <Button
        android:id="@+id/setuserinfo"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="setUserInfo" />

    <Button
        android:id="@+id/getalarminfo"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="getAlarmInfo" />

    <Button
        android:id="@+id/setuserid"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="setUserId" />

    <Button
        android:id="@+id/reset"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:text="reset" />

    <TextView
        android:id="@+id/tv_return"
        android:layout_width="fill_parent"
        android:layout_height="150dp" />

</LinearLayout>
